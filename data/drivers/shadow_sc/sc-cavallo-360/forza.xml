<!DOCTYPE params SYSTEM "../../../../src/libs/tgf/params.dtd">

<params name="sc-cavallo-360" type="template" mode="mw">
  	<section name="Features">
    	<attstr name="tire temperature and degradation" in="yes" val="no"/>
  	</section>

	<section name="Brake System">
		<attnum name="front-rear brake repartition" min="0.4" max="0.7" val="0.525"/>
		<attnum name="max pressure" unit="kPa" min="5000" max="50000" val="40000"/>
	</section>
	
	<section name="private">
		<attnum name="shift" val="0.99"/>
		<attnum name="fuel per meters" val="0.00048"/>
		<attnum name="kz scale" val="0.4"/>
		<attnum name="bump factor" val="0.2"/>
		<attnum name="bump factor left" val="0.134"/>
		<attnum name="bump factor right" val="0.6"/>
		<attnum name="mu scale" val="1.05"/>
		<attnum name="factor 0" val="1.001"/>
		<attnum name="steer k acc" val="3"/>
		<attnum name="steer k dec" val="0"/>
		<attnum name="spd ctrl normal" val="1"/>
		<attnum name="spd ctrl traffic" val="4"/>
		<attnum name="spd ctrl extra" val="6"/>
		<attnum name="steer ctrl" val="0"/>
		<attnum name="full fuel" val= "0"/>
	</section>

</params>














